{
  "name": "SchwurblerControllers",
  "version": "1.0.3",
  "description": "Control Library for Midi Inputs",
  "keywords": "controller, midi, input, rotary, button, poti",
  "repository": {
    "type": "git",
    "url": "https://github.com/mommel/schwurblercontrollers"
  },
  "authors": [{
    "name": "Manuel Braun",
    "email": "mommel.dev@gmail.com",
    "url": "https://www.mommel.com/",
    "maintainer": true
  }],
  "license": "CC-BY-NC-SA-4.0",
  "homepage": "https://github.com/mommel/hs-lr-midi-schwurbler/",
  "export": {
    "include":
    [
        "src/*.[c]pp",
        "include/*.h",
        "examples/*",
        "*/*/*.h"
    ]
  },
  "exclude": [
    ".pio/",
    ".vscode/",
    ".clang-format",
    "utils",
    "test",
    "docs"
  ],
  "dependencies":
  {
    "dxinteractive/ResponsiveAnalogRead": "^1.2.1",
    "mathertel/RotaryEncoder": "^1.3.0",
    "thomasfredericks/Bounce2": "^2.55"
  },
  "examples": [
    {
        "name": "ControlButtons",
        "base": "examples/ControlButton",
        "files": ["ControlButton.cpp"]
    },
    {
      "name": "ControlPotis",
      "base": "examples/ControlPoti",
      "files": ["ControlPoti.cpp"]
    }
  ],
  "frameworks": "arduino",
  "platforms": "*"
}
